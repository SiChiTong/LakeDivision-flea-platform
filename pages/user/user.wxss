/**index.wxss**/

/* 本人的信息栏 */
.top{
  height: 200rpx;
  border-radius: 30rpx;
  background-color: #FFFAFA;
  
}

.top .nav{
  display: flex;
  justify-content: space-between;
  margin: 0 auto;
  padding: 25rpx 0;
  height: 150rpx;
  width: 700rpx;
}

/* 头像，昵称栏 */
.top .nav .nav-right{
  display: flex;
  height: 100%;
  width: 400rpx;
}

/* 用户头像 */
.top .nav .nav-right .headPic{
  /* border: 1px solid rgb(255, 136, 0); */
  height: 150rpx;
  width: 150rpx;
  background-size: 100%;
  background-image: url("data:image/png;base64,");
}

/* 用户信息 */
.top .nav .nav-right .name{
  padding: 10rpx;
  height: 150rpx;
  width: 230rpx;
}
.top .nav .nav-right .name .name1,
.top .nav .nav-right .name .name2,
.top .nav .nav-right .name .name3{
  height: 46rpx;
  width: 250rpx;
}

/* 用户名 */
.top .nav .nav-right .name .name2{
  font-size: 25rpx;
  line-height: 46rpx;
}

/* 个人主页选项 */
.top .nav .nav-right .name .name3{
  width: 120rpx;
  font-size: 25rpx;
  text-align: center;
  line-height: 46rpx;
  background-color: #F5FFFA;
  border-radius: 30rpx;
}

.top .nav .nav-left{
  /* border: 1px solid rgb(0, 247, 255); */
  height: 100%;
  width: 300rpx;
}

/* 设置按钮 */
.top .nav .nav-left .setup{
  /* border: 1rpx solid rgb(226, 122, 36); */
  float: right;
  height: 50rpx;
  width: 50rpx;
  background-size: 100%;
  background-image: url("data:image/png;base64,");
}


/* 关于点赞和关注的地方 */
.fans{
  height: 100rpx;
  display: flex;
  justify-content: space-around;
  border-radius: 30rpx;
  background-color: #FFFAFA;
  margin: 20rpx 0
}
.fans .agree,
.fans .like,
.fans .myFans{
  height: 100rpx;
  width: 350rpx;
  text-align: center;
  line-height: 100rpx;
}

/* 卖在咸鱼 */
.shop{
  height: 300rpx;
  border-radius: 30rpx;
  background-color: #FFFAFA;
}
/* 顶部文字 */
.shop .shop-nav{
  border-radius: 30rpx;
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 30rpx;
}
.shop .shop-bottom{
  display: flex;
  justify-content: space-around;
  height: 220rpx;
}

/* 卖在咸鱼的两个选项 */
.shop .shop-bottom .shop-bottom1,
.shop .shop-bottom .shop-bottom2{
  margin: 20rpx 0;
  /* border: 1rpx solid rgb(226, 122, 36); */
  text-align: center;
  height: 180rpx;
  width: 180rpx;
}
.shop .shop-bottom .shop-bottom1 .pic,
.shop .shop-bottom .shop-bottom2 .pic{
  /* border: 1rpx solid rgb(226, 122, 36); */
  margin: 20rpx auto;
  height: 80rpx;
  width: 80rpx;
  background-size: 100%;
  background-image: url("data:image/png;base64,");
}

.shop .shop-bottom .shop-bottom1 .text,
.shop .shop-bottom .shop-bottom2 .text{
  height: 40rpx;
  width: 150rpx;
  margin: 10rpx auto;
}

/* 玩在咸鱼 */
.type{
  height: 460rpx;
  margin: 20rpx 0;
  border-radius: 30rpx;
  background-color:	#FFFAFA;
}
/* 顶部文字 */
.type .type-nav{
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 30rpx;
}
/* 三排的选项 */
.type .things1,
.type .things2,
.type .things3{
  /* border: 1rpx solid black; */
  display: flex;
  justify-content: space-around;
  height: 100rpx;
  padding: 10rpx 0;
}
/* 三排选项的每一排按钮 */
.type .things1 .thing1,
.type .things1 .thing2,
.type .things1 .thing3,
.type .things1 .thing4,
.type .things2 .thing1,
.type .things2 .thing2,
.type .things2 .thing3,
.type .things2 .thing4{
  /* border: 1rpx solid black; */
  height: 100rpx;
  width: 100rpx;
}

.type .things1 .thing1 .pic1,
.type .things1 .thing2 .pic1,
.type .things1 .thing3 .pic1,
.type .things1 .thing4 .pic1,
.type .things2 .thing1 .pic1,
.type .things2 .thing2 .pic1,
.type .things2 .thing3 .pic1,
.type .things2 .thing4 .pic1{
  /* border: 1px solid rgb(226, 122, 36); */
  margin: 0 auto;
  height: 60rpx;
  width: 60rpx;
  background-size: 100%;
  background-image: url("data:image/png;base64,");
}

.type .things1 .thing1 .text1,
.type .things1 .thing2 .text1,
.type .things1 .thing3 .text1,
.type .things1 .thing4 .text1,
.type .things2 .thing1 .text1,
.type .things2 .thing2 .text1,
.type .things2 .thing3 .text1,
.type .things2 .thing4 .text1{
  font-size: 20rpx;
  height: 30rpx;
  width: 100%;
  line-height: 30rpx;
  text-align: center;
}

.type .things3{
  height: 80rpx;
  border-radius: 30rpx;
  margin: 0 20rpx;
  background-color: #F5FFFA;
}

.type .things3 .pic1{
  /* border: 1px solid rgb(226, 122, 36); */
  height: 70rpx;
  width: 70rpx;
  background-size: 100%;
  background-image: url("data:image/png;base64,");
}
.type .things .text1{
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
}

